{
    "version": 2,
    "builds" : [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/v1/user/(.*)", "dest": "/src/routes/userRoutes"},
        { "src": "/v1/pass/(.*)", "dest": "/src/routes/passRoutes"},
        { "src": "/v1/auth/(.*)", "dest": "/src/routes/authRoutes"}
    ]
}